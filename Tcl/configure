#else
		seek = N_TXTOFF (foo);
#endif
		flag = (foo.a_midmag == OMAGIC);
		return foo.a_text + foo.a_data + foo.a_bss + foo.a_entry;
	    
; return 0; }
EOF
if eval $ac_compile; then
  rm -rf conftest*
  tcl_ok=usable
else
  rm -rf conftest*
  tcl_ok=unusable
fi
